<!--index.ejs-->

<body>
    <div class="row">
        <div class="card col-12 col-lg-12 col-sm-12">
            <table class="table table-striped">
                    <% User.forEach( function(model,i) { %>
                        <form action="/RERS/delete/<%=model.id%>" method="POST">
                            <tr>
                                <td> <label> <%=i+1%></label> </td>
                                <td> <a ><%=model.username%></a></td>
                            </tr>
                            <% }); %>
             </table>
            </form>
        </div>
    </div>
</body>